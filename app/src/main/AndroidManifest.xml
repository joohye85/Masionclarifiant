<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.masionclarifiant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:allowClearUserData="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Masion Clarifiant"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:name=".MyApplication">
        <activity android:name=".MyskinActivity" />
        <activity android:name=".CombiActivity" />
        <activity android:name=".MypageActivity" />
        <activity android:name=".Mypage2Activity" />
        <activity android:name=".FindActivity" />
        <activity android:name=".FindIngredientActivity"/>
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoadingActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".SelectActivity" />
        <activity android:name=".SelectResultActivity" />
        <activity android:name=".DiagnoseStart" />
        <activity android:name=".DiagnoseMeasure"/>
        <activity android:name=".DiagnoseFin"/>
        <activity android:name=".DiagnoseResult"/>
        <activity android:name=".DiagnoseRecommend"/>
    </application>

</manifest>